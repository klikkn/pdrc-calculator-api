{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "orders"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "make": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "client": {
      "type": "string"
    },
    "classIndex": {
      "type": "integer"
    },
    "items": {
      "type": "json"
    },
    "price": {
      "type": "integer"
    },
    "is_deleted": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "date": {
      "type": "datetime"
    }
  }
}
